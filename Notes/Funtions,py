def factoral(number):
    total = 1
    for i in range(1, number +1):
        total *= i 
    return total

for num in range(1,10):
    print(f"the factoral of {num} is {factoral(num)}")

    '''#ex 2
    for num in range(1,100):
    print(f"the factoral of {num} is {factoral(num)}")'''

    name = "Ms. LaRose"

    def admin():
